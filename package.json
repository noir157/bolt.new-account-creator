{
  "name": "bolt-account-creator",
  "version": "1.0.0",
  "description": "Criador de contas automatizado para Bolt.new",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "package": "electron-builder --dir"
  },
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "puppeteer": "^19.7.0"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "com.seunome.bolt-account-creator",
    "productName": "Bolt Account Creator",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "directories": {
      "output": "dist"
    }
  }
}